<?xml version="1.0"?>

<gen>
    <domains>
        <!--
        table_name : 데이터베이스 Table (SQL만들때 사용)
        table_class_name : 도메인 클래스 형식 (클래스 만들때 사용)
        table_func_name : 도메인 변수 형식 (각종 변수에 사용)

        field_name : 데이터베이스 Column (SQL만들때 사용)
        field_class_name : 프로퍼티 클래스 형식 (set, get 함수등에서 사용)
        field_func_name : 프로퍼티 변수 형식 (각종 변수에 사용)
        -->
        <domain table_name="board" table_comment="" table_class_name="Board" table_func_name="board" package="board">
            <field field_name="seq" field_comment="" field_func_name="seq" pk="Y"/>
            <field field_name="write" field_comment="" field_func_name="write"/>
            <field field_name="subject" field_comment="" field_func_name="subject"/>
            <field field_name="content" field_comment="" field_func_name="content"/>
            <field field_name="register_date" field_comment="" field_func_name="register_date"/>
        </domain>
    </domains>
</gen>